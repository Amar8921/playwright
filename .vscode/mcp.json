{
  "servers": {
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ],
      "cwd": "${workspaceFolder}",
      "env": {},
      "type": "stdio"
    },
    "hello": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-hello@latest"
      ],
